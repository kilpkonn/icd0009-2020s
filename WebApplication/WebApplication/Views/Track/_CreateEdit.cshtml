@model WebApplication.Models.Track.CreateEditViewModel;

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="Track!.Id" value="@(Model?.Track?.Id ?? Guid.NewGuid())"/>
<div class="form-group">
    <label asp-for="Track!.StartTimestamp" class="control-label"></label>
    <input asp-for="Track!.StartTimestamp" class="form-control" />
    <span asp-validation-for="Track!.StartTimestamp" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Track!.EndTimestamp" class="control-label"></label>
    <input asp-for="Track!.EndTimestamp" class="form-control" />
    <span asp-validation-for="Track!.EndTimestamp" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Track!.Distance" class="control-label"></label>
    <input asp-for="Track!.Distance" class="form-control" />
    <span asp-validation-for="Track!.Distance" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Track!.CarId" class="control-label"></label>
    <select asp-for="Track!.CarId" class="form-control" asp-items="Model!.Cars"></select>
    <span asp-validation-for="Track!.CarId" class="text-danger"></span>
</div>