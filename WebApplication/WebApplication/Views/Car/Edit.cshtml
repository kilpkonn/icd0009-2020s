@using Resource.Base
@using Resources
@model WebApplication.Models.Car.CreateEditViewModel

@{
    ViewData["Title"] = Common.Edit;
}

<h1>@Common.Edit</h1>

<h4>@Shared.Car</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <partial name="_CreateEdit" model="Model"/>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Common.Back</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
