@model WebApplication.Models.Car.CreateEditViewModel;

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="Car!.Id" value="@(Model.Car?.Id ?? Guid.NewGuid())"/>
<div class="form-group">
    <label asp-for="Car!.CarTypeId" class="control-label"></label>
    <select asp-for="Car!.CarTypeId" class="form-control" asp-items="Model.CarTypeOptions"></select>
    <span asp-validation-for="Car!.CarTypeId" class="text-danger"></span>
</div>