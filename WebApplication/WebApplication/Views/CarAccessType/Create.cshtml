@using Resource.Base
@using Resources
@model BLL.App.DTO.CarAccessType

@{
    ViewData["Title"] = Common.Create;
}

<h1>@Common.Create</h1>

<h4>@Shared.CarAccessType</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <partial name="_CreateEdit" model="Model"/>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">@Common.Back</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
