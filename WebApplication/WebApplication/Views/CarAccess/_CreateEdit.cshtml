@model WebApplication.Models.CarAccess.CreateEditViewModel;

<div asp-validation-summary="ModelOnly" class="text-danger"></div>

<input type="hidden" asp-for="CarAccess!.Id"/>
<div class="form-group">
    <label asp-for="CarAccess!.CarId" class="control-label"></label>
    <select asp-for="CarAccess!.CarId" class="form-control" asp-items="Model.CarOptions"></select>
</div>
<div class="form-group">
    <label asp-for="CarAccess!.CarAccessTypeId" class="control-label"></label>
    <select asp-for="CarAccess!.CarAccessTypeId" class="form-control" asp-items="Model.CarAccessTypeOptions"></select>
</div>