@model WebApplication.Models.CarErrorCode.CreateEditViewModel;

<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="CarErrorCode!.Id" value="@(Model.CarErrorCode?.Id ?? Guid.NewGuid())"/>
<div class="form-group">
    <label asp-for="CarErrorCode!.CanId" class="control-label"></label>
    <input asp-for="CarErrorCode!.CanId" class="form-control" />
    <span asp-validation-for="CarErrorCode!.CanId" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="CarErrorCode!.CanData" class="control-label"></label>
    <input asp-for="CarErrorCode!.CanData" class="form-control" />
    <span asp-validation-for="CarErrorCode!.CanData" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="CarErrorCode!.CarId" class="control-label"></label>
    <select asp-for="CarErrorCode!.CarId" class="form-control" asp-items="Model.CarOptions"></select>
</div>
